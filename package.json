{
	"name": "n8n-nodes-forest",
	"version": "0.1.1",
	"description": "n8n community node for Forest",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "Vincent MoliniÃ©",
		"email": "molinie.vincent@gmail.com"
	},
	"repository": {
		"type": "git",
		"url": ""
	},
	"engines": {
		"node": ">=18.10",
		"pnpm": ">=9.1"
	},
	"main": "index.js",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"build": "node esbuild.config.mjs && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/ForestMcpApi.credentials.js",
			"dist/credentials/ForestMcpOAuth2Api.credentials.js"
		],
		"nodes": [
			"dist/nodes/Forest/Forest.node.js"
		]
	},
	"devDependencies": {
		"@modelcontextprotocol/sdk": "^1.12.1",
		"@n8n/eslint-plugin-community-nodes": "^0.7.0",
		"@types/json-schema": "^7.0.15",
		"@types/node": "^22.0.0",
		"@typescript-eslint/parser": "^7.15.0",
		"esbuild": "^0.27.3",
		"eslint": "^8.56.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.1",
		"gulp": "^4.0.2",
		"jsonc-eslint-parser": "^2.4.2",
		"n8n-workflow": "*",
		"prettier": "^3.3.2",
		"typescript": "^5.5.3",
		"zod": "^3.24.0"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
